CONFIG:
  BATCH_INFO: sbatch  $SBATCH_TEMPLATES/SBATCH_Midway2a.TEMPLATE 60
  PATH_SNDATA_SIM: /project2/rkessler/SURVEYS/LSST/SIM
  # RESET_CIDOFF: 2
  SIMGEN_INFILE_NONIa:
  - SIMGEN_TEMPLATE_LSST_EUCLID.INPUT   ## I changed this to your new LSST+EUCLID input
  NGEN_UNIT: 0.05
  H_0: 70
  GENPREFIX: EULCID_LSSTDDF
  FORMAT_MASK: 112
  RANSEED_REPEAT: 20 13773

GENVERSION_LIST:
 - GENVERSION: LSSTDDF_SNIa_SIMLIB
   GENOPT:
     INPUT_FILE_INCLUDE: model_config/sn_ia_salt2_g10.input
     HOSTLIB_WGTMAP_FILE: model_config/DES_WGTMAP_MassSFR_Wiseman2021.HOSTLIB
     HOSTLIB_SCALE_PROPERTY_ERR: 0.0(LOGMASS),0.0(LOGSFR),0.0(LOGsSFR),0.0(COLOR)
     HOSTLIB_MSKOPT: 258
     HOSTLIB_FILE: model_config/DES_SVA2+LOGMASS_LOGSFR_Sullivan10.HOSTLIB
     GENPDF_FILE: model_config/S3G10_GENPDF.DAT
     GENTYPE: 1
     SIMLIB_FILE: SIMLIB_baseline_v2.0_10yrs_DDF.simlib.COADD  SOLID_ANGLE 0.0091 # 30 sq deg ONLY is the overlap between Euclid and LSST-DDF 
     GENPDF_OPTMASK: 1
     GENFILTERS:   ugrizy

 - GENVERSION: EUCLIDLSSTDDF_SNIa_SIMLIB
   GENOPT:
     INPUT_FILE_INCLUDE: model_config/sn_ia_salt2_g10.input
     HOSTLIB_WGTMAP_FILE: model_config/DES_WGTMAP_MassSFR_Wiseman2021.HOSTLIB
     HOSTLIB_SCALE_PROPERTY_ERR: 0.0(LOGMASS),0.0(LOGSFR),0.0(LOGsSFR),0.0(COLOR)
     HOSTLIB_MSKOPT: 258
     HOSTLIB_FILE: model_config/DES_SVA2+LOGMASS_LOGSFR_Sullivan10.HOSTLIB
     GENPDF_FILE: model_config/S3G10_GENPDF.DAT
     GENTYPE: 1
     SIMLIB_FILE: SIMLIB_baseline_v2.0_10yrs_DDF_wEUCLID.simlib.COADD SOLID_ANGLE 0.0091 # 30 sq deg ONLY is the overlap between Euclid and LSST-DDF
     GENFILTERS:   ugrizyVYJH
     GENPDF_OPTMASK: 1
     SIMGEN_DUMP: 19 CID GENTYPE GENZ PEAKMAG_g PEAKMAG_r PEAKMAG_i PEAKMAG_z PEAKMAG_u SNRMAX_r PEAKMAG_V PEAKMAG_J PEAKMAG_H PEAKMAG_Y PEAKMAG_y SNRMAX_r SNRMAX_J SNRMAX_H SNRMAX_Y SNRMAX_V

 - GENVERSION: EUCLIDLSSTDDF_SNIa_SIMLIB_BS20
   GENOPT:
     INPUT_FILE_INCLUDE: model_config/sn_ia_salt2_bs20.input
     HOSTLIB_WGTMAP_FILE: model_config/DES_WGTMAP_MassSFR_Wiseman2021.HOSTLIB
     HOSTLIB_SCALE_PROPERTY_ERR: 0.0(LOGMASS),0.0(LOGSFR),0.0(LOGsSFR),0.0(COLOR)
     HOSTLIB_MSKOPT: 258
     HOSTLIB_FILE: model_config/DES_SVA2+LOGMASS_LOGSFR_Sullivan10.HOSTLIB
     GENTYPE: 1
     GENMAG_SMEAR_SCALE: 0.0001
     GENMAG_SMEAR_SCALE(c): 0,0
     GENPDF_OPTMASK: 1
     GENPDF_FILE: model_config/S3BS20_GENPDF.DAT
     SIMLIB_FILE: SIMLIB_baseline_v2.0_10yrs_DDF_wEUCLID.simlib.COADD SOLID_ANGLE 0.0091 # 30 sq deg ONLY is the overlap between Euclid and LSST-DDF
     GENFILTERS:   ugrizyVYJH
     SIMGEN_DUMP: 19 CID GENTYPE GENZ PEAKMAG_g PEAKMAG_r PEAKMAG_i PEAKMAG_z PEAKMAG_u SNRMAX_r PEAKMAG_V PEAKMAG_J PEAKMAG_H PEAKMAG_Y PEAKMAG_y SNRMAX_r SNRMAX_J SNRMAX_H SNRMAX_Y SNRMAX_V

 - GENVERSION: EUCLIDLSSTDDF_SNIa_SIMLIB_gMS
   GENOPT:
     INPUT_FILE_INCLUDE: model_config/sn_ia_salt2_g10.input
     HOSTLIB_WGTMAP_FILE: model_config/DES_WGTMAP_MassSFR_Wiseman2021_STEP.HOSTLIB
     HOSTLIB_SCALE_PROPERTY_ERR: 0.0(LOGMASS),0.0(LOGSFR),0.0(LOGsSFR),0.0(COLOR)
     HOSTLIB_FILE: model_config/DES_SVA2+LOGMASS_LOGSFR_Sullivan10.HOSTLIB
     HOSTLIB_MSKOPT: 262
     GENPDF_FILE: model_config/S3G10_GENPDF.DAT
     GENTYPE: 1
     GENPDF_OPTMASK: 1
     SIMLIB_FILE: SIMLIB_baseline_v2.0_10yrs_DDF_wEUCLID.simlib.COADD SOLID_ANGLE 0.0091 # 30 sq deg ONLY is the overlap between Euclid and LSST-DDF
     GENFILTERS:   ugrizyVYJH
     SIMGEN_DUMP: 19 CID GENTYPE GENZ PEAKMAG_g PEAKMAG_r PEAKMAG_i PEAKMAG_z PEAKMAG_u SNRMAX_r PEAKMAG_V PEAKMAG_J PEAKMAG_H PEAKMAG_Y PEAKMAG_y SNRMAX_r SNRMAX_J SNRMAX_H SNRMAX_Y SNRMAX_V

#END_YAML
